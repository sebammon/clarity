{
  "name": "clarity",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://sebammon.github.io/clarity",
  "scripts": {
    "build": "tsc && vite build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "dev": "vite --port 8000",
    "format": "prettier --write src",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@tanstack/react-query": "^4.14.3",
    "axios": "^1.1.3",
    "dexie": "^3.2.2",
    "framer-motion": "^10.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "remove-markdown": "^0.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
